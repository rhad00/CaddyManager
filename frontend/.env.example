# API Configuration
VITE_API_URL=/api
VITE_WS_URL=/api

# Authentication
# Key used to store the JWT token in localStorage/sessionStorage
# Change this if you want to use a different storage key
VITE_JWT_STORAGE_KEY=caddy_manager_token

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_DARK_MODE=true

# Monitoring Configuration
VITE_METRICS_REFRESH_INTERVAL=30000  # 30 seconds
VITE_HEALTH_CHECK_INTERVAL=60000     # 1 minute
