{
  "admin": {
    "listen": "0.0.0.0:2019"
  },
  "apps": {
    "http": {
      "metrics": {
        "per_host": true
      },
      "servers": {
        "srv0": {
          "automatic_https": {
            "disable_redirects": true
          },
          "listen": [
            ":443"
          ],
          "routes": [
            {
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "body": "No routes configured yet (HTTPS)",
                          "handler": "static_response",
                          "status_code": 503
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        "srv1": {
          "automatic_https": {
            "disable_redirects": true
          },
          "listen": [
            ":80"
          ],
          "routes": [
            {
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "body": "No routes configured yet",
                          "handler": "static_response",
                          "status_code": 503
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  },
  "storage": {
    "module": "file_system",
    "root": "/data"
  }
}